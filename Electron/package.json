{
  "name": "Ezaix-Endpoint-Metrics",
  "version": "2.0.0",
  "author": {
    "name": "Ezaix",
    "email": "info@cybersealai.com",
    "url": "https://ezaix.com/",
    "company": "Ezaix"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux"
  },
  "build": {
    "appId": "com.ezaix.endpointmetrics",
    "productName": "Ezaix Endpoint-Metrics",
    "directories": {
      "output": "release-builds"
    },
    "files": [
      "build/**/*",
      "main.js",
      "preload.js",
      "package.json",
      "icons/**/*",
      "dataMiners/**/*",
      "ipcHandlers/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "icon": "./Icons/Modified.ico"
    },
    "mac": {
      "icon": "./Icons/EndpointMetricsv1.icns"
    },
    "linux": {
      "icon": "./Icons/Modified.png"
    }
  },
  "dependencies": {
    "axios": "^1.6.7",
    "battery-level": "^3.0.0",
    "electron-store": "^8.1.0",
    "fast-speedtest-api": "^0.3.2",
    "fs": "^0.0.1-security",
    "luxon": "^3.4.4",
    "node-fetch": "^3.3.2",
    "ping": "^0.4.4",
    "sqlite3": "^5.1.6",
    "systeminformation": "^5.21.24",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "electron": "^28.2.2",
    "electron-builder": "^24.13.3"
  }
}
