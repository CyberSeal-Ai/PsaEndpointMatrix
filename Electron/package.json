{
  "name": "Endpoint-Metrics-Testing-v2.1.4",
  "version": "2.1.4",
  "author": {
    "name": "Ezaix Testing v2.1",
    "email": "sudarsa@ezaix.com",
    "url": "ezaixEndpointTesting.com",
    "company": "Ezaix Technologies"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux"
  },
  "build": {
    "appId": "com.v2.1.4ezaix.endpointmetrics",
    "productName": "Endpoint-Metrics-Ezaix V2.1.4",
    "directories": {
      "output": "release-builds-7-mac"
    },
    "files": [
      "build/**/*",
      "main.js",
      "preload.js",
      "package.json",
      "icons/**/*",
      "dataMiners/**/*",
      "ipcHandlers/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "icon": "./Icons/Modified.ico"
    },
    "mac": {
      "icon": "./Icons/mac.icns",
      "category": "public.app-category.utilities",
      "target": [
        {
          "target": "dmg",
          "arch": ["x64"]
        }
      ],
      "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
      "compression": "maximum"
    },
    "dmg": {
      "icon": "./Icons/mac.icns",
      "iconSize": 100,
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "window": {
        "width": 540,
        "height": 380
      }
    },
    "linux": {
      "icon": "./Icons/Modified.png"
    }
  },
  "dependencies": {
    "axios": "^1.6.7",
    "battery-level": "^3.0.0",
    "electron-store": "^8.1.0",
    "ezaixlogo": "^1.0.0",
    "fast-speedtest-api": "^0.3.2",
    "fs": "^0.0.1-security",
    "luxon": "^3.4.4",
    "node-fetch": "^3.3.2",
    "ping": "^0.4.4",
    "sqlite3": "^5.1.6",
    "systeminformation": "^5.21.24",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "electron": "^28.2.2",
    "electron-builder": "^24.13.3"
  }
}
